<robot name="3DOF-WORM">
    <link name="base"/>

    <joint name="joint-0" type="fixed">
        <parent link="base"/>
        <child link="link-1"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <link name="link-1">
        <inertial>
            <origin xyz="0 0 0.0725" rpy="0 0 0"/>
            <mass value="1.59"/>
            <inertia ixx="0.010" ixy="0" ixz="0" iyy="0.010" iyz="0" izz="0.011"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.045" rpy="0 0 0"/>
            <geometry>
                <mesh filename="../Meshes/link-1.gltf"/>
            </geometry>
        </visual>

        <collision name="link-1-base-collision">
            <origin xyz="0 0 0.035" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.11" radius="0.075"/>
            </geometry>
        </collision>

        <collision name="link-1-motor-collision">
            <origin xyz="0.008 0 0.145" rpy="0 1.57 0"/>
            <geometry>
                <cylinder length="0.075" radius="0.06"/>
            </geometry>
        </collision>
    </link>

    <joint name="joint-1" type="revolute">
        <parent link="link-1"/>
        <child link="link-2"/>
        <origin xyz="0 0 0.145" rpy="0 0 0" />
        <axis xyz="1 0 0" />
        <limit lower="-1.57" upper="1.57" effort="120" velocity="5.02654824"/>
        <dynamics damping="0.8487"/>
    </joint>

    <link name="link-2">
        <inertial>
            <origin xyz="0 0 0.1275" rpy="0 0 0"/>
            <mass value="1.59"/>
            <inertia ixx="0.011" ixy="0" ixz="0" iyy="0.011" iyz="0" izz="0.004"/>
        </inertial>

        <visual>
            <origin xyz="0.05 0 0" rpy="0 0 -1.57"/>
            <geometry>
                <mesh filename="../Meshes/link-2.gltf"/>
            </geometry>
        </visual>

        <collision name="link-2-base-collision">
            <origin xyz="0.01 0 0.085" rpy="0 0 0"/>
            <geometry>
                <box size="0.1 0.08 0.22"/>
            </geometry>
        </collision>

        <collision name="link-2-motor-collision">
            <origin xyz="0 0.008 0.255" rpy="1.57 0 0"/>
            <geometry>
                <cylinder length="0.075" radius="0.06"/>
            </geometry>
        </collision>
    </link>

    <joint name="joint-2" type="revolute">
        <parent link="link-2"/>
        <child link="link-3"/>
        <origin xyz="0 0 0.255" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit lower="-2.094" upper="2.094" effort="120" velocity="5.02654824"/>
        <dynamics damping="0.8487"/>
    </joint>

    <link name="link-3">
        <inertial>
            <origin xyz="0 0 0.11" rpy="0 0 0"/>
            <mass value="1.59"/>
            <inertia ixx="0.009" ixy="0" ixz="0" iyy="0.009" iyz="0" izz="0.004"/>
        </inertial>

        <visual>
            <origin xyz="0 0.05 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="../Meshes/link-3.gltf"/>
            </geometry>
        </visual>

        <collision name="link-3-base-collision">
            <origin xyz="0 0.01 0.07" rpy="0 0 0"/>
            <geometry>
                <box size="0.08 0.1 0.18"/>
            </geometry>
        </collision>

        <collision name="link-3-motor-collision">
            <origin xyz="0 -0.01 0.22" rpy="1.57 0 0"/>
            <geometry>
                <cylinder length="0.075" radius="0.06"/>
            </geometry>
        </collision>
    </link>

    <joint name="joint-3" type="revolute">
        <parent link="link-3"/>
        <child link="link-4"/>
        <origin xyz="0 0 0.22" rpy="0 0 0"/>
        <axis xyz="0 1 0" />
        <limit lower="-2.094" upper="2.094" effort="120" velocity="5.02654824"/>
        <dynamics damping="0.8487"/>
    </joint>

    <link name="link-4">
        <inertial>
            <origin xyz="0 0.015 0.2425" rpy="0 0 0"/>
            <mass value="5.00"/>
            <inertia ixx="0.107" ixy="0" ixz="0" iyy="0.103" iyz="0" izz="0.014"/>
        </inertial>

        <visual>
            <origin xyz="0 -0.05 0" rpy="0 0 1.57"/>
            <geometry>
                <mesh filename="../Meshes/link-4.gltf"/>
            </geometry>
        </visual>

        <collision name="link-3-base-collision">
            <origin xyz="0 -0.015 0.2" rpy="0 0 0"/>
            <geometry>
                <box size="0.11 0.15 0.45"/>
            </geometry>
        </collision>

        <collision name="link-1-uib-collision">
            <origin xyz="0 -0.01 0.46" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.08" radius="0.075"/>
            </geometry>
        </collision>
    </link>

    <!-- Transmission definitions -->
    <transmission name="tran-1">
        <robotNamespace>/3DOF-WORM</robotNamespace>
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="joint-1">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor-1">
            <mechanicalReduction>1</mechanicalReduction>
            <drake:gear_ratio value="64.0" />
            <drake:rotor_inertia value="0.00005645" />
        </actuator>
    </transmission>

    <transmission name="tran-2">
        <robotNamespace>/3DOF-WORM</robotNamespace>
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="joint-2">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor-2">
            <mechanicalReduction>1</mechanicalReduction>
            <drake:gear_ratio value="64.0" />
            <drake:rotor_inertia value="0.00005645" />
        </actuator>
    </transmission>

    <transmission name="tran-3">
        <robotNamespace>/3DOF-WORM</robotNamespace>
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="joint-3">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor-3">
            <mechanicalReduction>1</mechanicalReduction>
            <drake:gear_ratio value="64.0" />
            <drake:rotor_inertia value="0.00005645" />
        </actuator>
    </transmission>
</robot>